{
  "name": "college-resources-api",
  "version": "0.1.0",
  "description": "College Resources API Server",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "watch": "npm-run-all --sequential watch:prepare watch:run",
    "watch:prepare": "node -e \"const fs = require('fs'); fs.existsSync('./dist') || fs.mkdirSync('./dist')\"",
    "watch:run": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "npm run development -- --watch",
    "watch:server": "nodemon --inspect --watch dist ./dist/bundle.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "lint": "eslint --ext .js .",
    "fix": "eslint --fix --ext .js .",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/college-resources/api.git"
  },
  "author": "College Resources",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://gitlab.com/college-resources/api/issues"
  },
  "homepage": "https://gitlab.com/college-resources/api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.2",
    "apollo-server-express": "^2.8.1",
    "auth0": "^2.18.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.2.1",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "helmet": "^3.20.0",
    "jest": "^25.5.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "standard": "^13.1.0",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "val-loader": "^2.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "latest"
    }
  }
}
